{
    "name": "slender/configurator",
    "description": "Multi-format configuration loader",
    "type": "library",
    "license": "MIT",
    "version": "0.0.1",
    "homepage": "https://github.com/alanpich/Slender-Configurator",
    "keywords": [
        "slender",
        "config",
        "configuration",
        "loader",
        "yaml",
        "json",
        "ini"
    ],
    "authors": [
        {
            "name": "Alan Pich",
            "email": "alanp@native.com"
        }
    ],
    "suggest": {
        "symfony/yaml": "For using YAML configuration files"
    },
    "autoload": {
        "psr-4": {
            "Slender\\Configurator\\": "src"
        }
    },
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.4",
        "satooshi/php-coveralls": "~0.6",
        "ext-curl": "~0.0"
    }
}
